is io;

fn println(val :: String) :: Void {
  `
    std::cout << val << std::endl;
  `
}

fn println(val :: Char) :: Void {
  `
    std::cout << std::string(1, val) << std::endl;
  `
}

fn println(val :: Int) :: Void {
  `
    std::cout << std::to_string(val) << std::endl;
  `
}


fn println(val :: Bool) :: Void {
  `
    std::cout << std::boolalpha << val << std::endl;
  `
}
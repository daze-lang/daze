is List;

struct List<T> {
  contents :: []T
}

implement List {
  fn new<T>() :: Self {
    Self.contents = []T{}
  }

  fn push(value :: T) :: Void {
    Self.contents <- value;
  }

  fn contents() :: []T {
    ret Self.contents;
  }

  fn get(index :: Int) :: T {
    ret Self.contents[index.int()];
  }
}
fn greet(name :: String) :: String {
  ret "Hello, " + name;
}

fn say(s :: String) :: Void {
  out(s);
}

struct Person {
  name :: String
}

implement Person {
  fn get_name() :: String {
    ret self.name;
  }
}

fn get_person() :: Person {
  ret make Person("Mark");
}

fn main() :: Int {
  someone :: String := "Someone";
  someone |> greet |> say;

  someone2 := make Person("Mark");
  someone2 |> .get_name |> greet |> say;

  get_person |> .get_name |> greet |> say;
}
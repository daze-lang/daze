is main;

use "daze::io";
use "daze::strings";

fn hello() :: String {
  ret "hello";
}

fn main() :: Int {
  io:println("================");
  io:println(strings:lower("HELLO"));
  io:println(strings:upper("hello"));
  io:println(strings:sub("Hello", 0, 2));
  io:println(strings:index("Hello", "l"));
  io:println(strings:index("Hello", "g"));

  sum :: []String := {};
  sum <- hello();
}
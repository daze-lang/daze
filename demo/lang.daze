is main;

use "daze::io";
use "daze::strings";

struct Person {
  name :: io:IOStruct
}

fn something() :: String? {
  ret "hello";
}

fn main() :: Int {
  human := make Person(make io:IOStruct("Mark"));
  if (strings:contains("hello", "h")) {
    io:println(len("Contains"));
  }

  human.name = make io:IOStruct("Daze");
  io:println(human.name.name);
}
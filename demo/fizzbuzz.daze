is Main;

use "daze::Std";

fn fizzbuzz() :: Void {
  i = 0
  for (i < 20) {
    if (i % 15 == 0) {
      Std.out("FizzBuzz");
    } elif (i % 3 == 0) {
      Std.out("Fizz");
    } elif (i % 5 == 0) {
      Std.out("Buzz");
    } else {
      Std.out(i);
    }

    i++;
  }

}

fn main() :: Void {
  fizzbuzz();
}
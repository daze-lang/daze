is main;

struct Person {
  name :: String,
  age :: Int32
}

implement Person {
  fn new(name :: String, age :: Int32) :: Void {
    Self.name = name;
    Self.age = age;
  }

  fn name() :: String {
    ret Self.name;
  }
}

fn name() :: String {
  ret "hello";
}

fn main() :: Void {
  p = Person.new("Mark", 24);
  out("My name is #{p.name()}");
}
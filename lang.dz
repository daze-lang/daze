is main;

struct Person {
  name :: String,
  age :: Int32
}

implement Person {
  fn new(name :: String, age :: Int32) :: Void {
    Self.name = name;
    Self.age = age;
  }

  fn something() :: String {
    ret "hello there my friend.";
  }

  fn get_hello() :: String {
    ret Self.something();
  }
}

fn main() :: Void {
  p = Person.new("Mark", 24);
  out("My name is #{p.name()} and I'm #{p.age()}, also: #{p.get_hello()}");
}